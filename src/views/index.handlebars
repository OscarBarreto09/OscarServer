<h1>PRODUCTS</h1>

 

{{#each productos}}
 <div class="card mb-3">
  <img src="{{this.photo}}" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text">{{this.brand}}</p>
    <p class="card-text"><small class="text-body-secondary">${{this.price}}</small></p>
  </div>
</div>
{{/each}}

